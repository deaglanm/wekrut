
 
                    



<%= render 'layouts/header' %> 
 <div class="container">
            <div class="col-md-4 col-md-offset-4">
              <div class="form-container">
               
                  <div class="login-form">

          

           

          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

   <div class="form-group">
    
    <%= f.email_field :email, autofocus: true, :placeholder => 'Email address', :class => 'form-control login-field' %>
  </div> 

  <div class="form-group">
    <%= f.password_field :password, autocomplete: "off", :placeholder => 'Password', :class => 'form-control login-field' %></div>

   <div class="form-group">
    <%= f.password_field :password_confirmation, autocomplete: "off", :placeholder => 'Password Confrimation', :class => 'form-control login-field' %></div>

     <div class="form-group">
    <%= f.password_field :first_name, autocomplete: "off", :placeholder => 'First Name', :class => 'form-control login-field' %></div>

     <div class="form-group">
    <%= f.password_field :last_name, autocomplete: "off", :placeholder => 'Last Name', :class => 'form-control login-field' %></div>

      <div class="form-group">
    <%= f.select :industry_1, industry_choices %></div>

       <div class="form-group">
    <%= f.select :industry_2, industry_choices %></div>

         <div class="form-group">
            <%= f.select :speciality_1, speciality_choices %></div>
   

      <div class="form-group">
      <%= f.select :speciality_2, speciality_choices %></div>

  <div>
    <%= f.submit "Sign up", :class => 'btn btn-primary btn-lg btn-block'%></div>
<% end %>
            <a class="login-link" href="#">Already a Member?</a>
          </div>
        </div>
    </div>
        </div>
      </div>

